{
  "name": "ignite-react",
  "description": "monorepo for Rocketseat's ignite react stuff",
  "version": "0.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com:mrocha98/ignite-react.git"
  },
  "license": "BSD-3-Clause-Clear",
  "scripts": {
    "build": "pnpm -r --stream run build ",
    "clean:dist": "pnpm --parallel exec rimraf dist",
    "dev": "pnpm -r run dev",
    "postinstall": "husky install && pnpm run sort && pnpm run pkg:check",
    "pkg:check": "manypkg check",
    "pkg:fix": "manypkg fix",
    "sort": "pnpm --parallel exec sort-package-json"
  },
  "dependencies": {
    "@babel/core": "~7.16.0",
    "@manypkg/cli": "~0.19.1",
    "@swc/cli": "~0.1.55",
    "@swc/core": "~1.2.126",
    "@swc/jest": "~0.2.15",
    "@testing-library/jest-dom": "~5.15.0",
    "@testing-library/react": "~12.1.2",
    "@testing-library/react-hooks": "~7.0.2",
    "@types/express": "~4.17.13",
    "@types/jest": "~27.0.2",
    "@types/node": "~16.11.7",
    "@types/react": "~17.0.38",
    "@typescript-eslint/eslint-plugin": "~5.4.0",
    "@typescript-eslint/parser": "~5.4.0",
    "classnames": "~2.3.1",
    "concurrently": "~7.0.0",
    "eslint": "~7.32.0",
    "eslint-config-next": "~12.0.4",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-prettier": "~4.0.0",
    "eslint-plugin-testing-library": "~3.9.2",
    "husky": "~7.0.4",
    "jest": "~27.3.1",
    "next": "12.0.7",
    "prettier": "~2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "~3.0.2",
    "sort-package-json": "~1.53.1",
    "typescript": "~4.4.4"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "pnpm": {
    "overrides": {
      "glob-parent@<5.1.2": ">=5.1.2"
    }
  }
}
